--allman false
--binarygrouping none
--closingparen balanced
--commas always
--conflictmarkers reject
--decimalgrouping none
--elseposition same-line
--empty void
--exponentcase lowercase
--exponentgrouping disabled
--fractiongrouping disabled
--fragment false
--header ignore
--hexgrouping none
--hexliteralcase uppercase
--ifdef indent
--importgrouping alphabetized
--indent 2
--indentcase false
--linebreaks lf
--octalgrouping none
--operatorfunc spaced
--patternlet hoist
--ranges spaced
--self remove
--selfrequired 
--semicolons never
--stripunusedargs always
--trailingclosures 
--trimwhitespace always
--wraparguments after-first
--wrapcollections before-first
--xcodeindentation disabled
--header "{file}\nCopyright (c) {year} Jerome Hsieh. All rights reserved.\nCreated by Jerome Hsieh on {created}."
